#ifndef _MYCLASS_HPP_
#define _MYCLASS_HPP_

class MyClass
{
public:
  MyClass(int i);
  MyClass();
  MyClass& operator = (MyClass &rht);
  bool operator < (MyClass &rht);

  MyClass& operator = (int pvalue){m_value = pvalue;}
  voi set(int pvalue){m_value = pvalue;}
  in get(){return m_value;}

private:
  int m_value;
};

#endif
